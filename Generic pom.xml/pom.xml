							<!-- Properties for mqsiapplybaroverride -->
							<barFile>${barFile}</barFile>
							<overrideFile>${overrideFile}</overrideFile>
							<traceFileName>${traceFileName}</traceFileName>
							<applicationName>${applicationName}</applicationName>
							<outputFile>${outputFile}</outputFile>
							<libraryName>${libraryName}</libraryName>
							<directManualOverrides>${directManualOverrides}</directManualOverrides>
							<recursion>${recursion}</recursion>
							<manualUsingPropertyfile>${manualUsingPropertyfile}</manualUsingPropertyfile>
							
							<!-- Properties for mqsideploybar -->
							<integrationNode>${integrationNode}</integrationNode>
							<integrationServer>${integrationServer}</integrationServer>
							<integrationNodeFileName>${integrationNodeFileName}</integrationNodeFileName>
							<isIndependentIntegrationServerPresent>${isIndependentIntegrationServerPresent}</isIndependentIntegrationServerPresent>
							<ipAddress>${ipAddress}</ipAddress>
							<port>${port}</port>
							<barFile>${barFile}</barFile>
							<deployedObjects>${deployedObjects}</deployedObjects>
							<overeridePreviousDeployment>${overeridePreviousDeployment}</overeridePreviousDeployment>
							<traceFileName>${traceFileName}</traceFileName>
							<timeoutSecs>${timeoutSecs}</timeoutSecs>
							
							<!-- Properties for install -->
							<barPath>${barPath}</barPath>
							<localm2Dir>${localm2Dir}</localm2Dir>
		
							<!-- Properties for mqsireadbar -->
							<barFile>${barFile}</barFile>
							<traceFileName>${traceFileName}</traceFileName>
							<recursion>${recursion}</recursion>
							<overrideFile>${overrideFile}</overrideFile>
							<readBarOutputFile>${readBarOutputFile}</readBarOutputFile>
							<finalCompareOutputFile>${finalCompareOutputFile}</finalCompareOutputFile>
							
						</configuration>
						<goals>
							<goal>validateEnv</goal>
							<!--<goal>packagebar</goal>-->
							<!--<goal>createbar</goal>-->
							<!--<goal>installbar</goal>-->
							<!--<goal>verify</goal>-->
							<!--<goal>deploybar</goal>-->
							<goal>baroverride</goal>
						</goals>
					</execution>

				</executions>
			</plugin>
		</plugins>
	</build>
