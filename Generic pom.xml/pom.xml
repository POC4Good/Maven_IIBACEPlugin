	<distributionManagement>
    <repository>
        <id>central</id>
        <name>TP-releases</name>
        <url>http://localhost:8081/artifactory/libs-release-local</url>
    </repository>
	</distributionManagement>
	
	<build>
		<plugins>
			<plugin>
				<groupId>com.ace.plugin</groupId>
				<artifactId>IBMACEBuildPlugin</artifactId>
				<version>1.0-SNAPSHOT</version>
				<executions>
					<execution>
						 <id>ACEPluginExecution</id>
						<configuration>
							<!-- Properties for mqsiprofile -->
							<mqsiprofileLoc>${mqsiprofileLoc}</mqsiprofileLoc>
							
							<!-- Properties for Jfrog Artifactory -->
							<artifactoryUpload>${artifactoryUpload}</artifactoryUpload>
							<artifactoryDownload>${artifactoryDownload}</artifactoryDownload>
							<artifactoryUserName>${artifactoryUserName}</artifactoryUserName>
							<artifactoryPassword>${artifactoryPassword}</artifactoryPassword>
							<artifactoryUrl>${artifactoryUrl}</artifactoryUrl>
							<artifactoryBarVersion>${artifactoryBarVersion}</artifactoryBarVersion>
							<artifactoryPOM>${artifactoryPOM}</artifactoryPOM>
							<artifactoryBarDownloadPath>${artifactoryBarDownloadPath}</artifactoryBarDownloadPath>

							<!-- Properties for mqsicreatebar -->
							<workspace>${workspace}</workspace>
							<barPath>${barPath}</barPath>
							<appName>${appName}</appName>
							<traceFilePath>${traceFilePath}</traceFilePath>
							<skipError>${skipError}</skipError>
							<libName>${libName}</libName>
